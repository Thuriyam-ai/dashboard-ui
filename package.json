{
  "name": "frontend-boilerplate",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "NEXT_PUBLIC_ENV=development next dev",
    "build": "NEXT_PUBLIC_ENV=production next build",
    "build:ignore-lint": "NEXT_PUBLIC_ENV=production ESLINT_NO_DEV_ERRORS=true next build",
    "build:dev": "NEXT_PUBLIC_ENV=development next build",
    "build:prod": "NEXT_PUBLIC_ENV=production next build",
    "build:staging": "NEXT_PUBLIC_ENV=staging next build",
    "build:beta": "NEXT_PUBLIC_ENV=beta next build",
    "build:qa": "NEXT_PUBLIC_ENV=qa next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "prepare": "husky || true",
    "build:gh-pages": "NODE_ENV=production NEXT_PUBLIC_ENV=production next build",
    "deploy:gh-pages": "npm run build:gh-pages && touch out/.nojekyll && git add out/ && git commit -m 'Deploy to GitHub Pages' && git subtree push --prefix out origin gh-pages"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@mui/x-charts": "^8.11.3",
    "@mui/x-data-grid": "^8.11.3",
    "next": "15.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.36.0",
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9.19.0",
    "@types/node": "22.18.6",
    "@types/react": "19.1.13",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "^15.1.6",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-jsdoc": "^50.6.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-typescript-enum": "^2.1.0",
    "eslint-plugin-unicorn": "^57.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "pretty-quick": "^4.0.0",
    "sass": "^1.92.1",
    "typescript": "5.9.2",
    "typescript-eslint": "^8.21.0"
  }
}
